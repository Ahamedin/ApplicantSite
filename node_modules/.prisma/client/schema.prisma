generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model blocks {
  id          Int      @id @default(autoincrement())
  name        String
  price       Float
  image       String
  updatedAt   DateTime @default(now()) @updatedAt
  description String?  @db.VarChar(300)
}

model booking {
  id      Int    @id @default(autoincrement())
  name    String
  mobile  String
  message String
}
